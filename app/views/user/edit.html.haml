.listeditPage
  .container
    = form_with model: @user, url: { action: :update }, html: { class: 'listForm' }, local: true do |f|
      - if @user.errors.any?
        %p.text-danger タイトルは1～255文字以内で入力してください
      = f.label :name
      = f.text_field :name, class: "form-control listName", placeholder: "リスト名"
      .text-center= f.submit "編集する", class: "submitBtn"
